{
  "manifest_version" : 2,
  "name" : "SS-formShortcuts",
  "version" : "0.1",
  "description" : "A chrome extension for form completion.",
  "browser_action" : {
    "default_icon" : "icon/icon19.png",
    "default_popup" : "popup.html",
    "default_title" : "SS-formCompletion"
  },
  "options_page" : "options.html",
  "permissions" : [
    "activeTab",
    "storage"
  ]
}
